/**index.wxss**/
/*搜索样式*/
.search{
  width: 100%;
  display: flex;
  justify-content: center;
  position: relative;
  margin-top: 18rpx;
  margin-bottom: 21rpx;
}
.search-navigator{
  width: 690rpx;
  height: 60rpx;
  border-radius: 33rpx;
}
.search-navigator input{
  width: 619rpx;
  height: 60rpx;
  background-color: #EBF0F4;
  border-radius: 33rpx;
  font-size: 22.8rpx;
  padding-left: 71rpx;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: 4% center;
  background-size: 5%;
}

/*搜索样式结束*/
/* 导航栏样式 */
.nav{
  width: 100%;
  font-size: 31.25‬‬rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 25rpx;
}
.nav .nav-name{
  width: 90rpx;
  height: 50rpx;
  text-align: center;
  font-size: 31.25rpx;
}
.nav .nav-show{
  color: #379FF6;
  font-size: 37.5rpx;
  border-bottom: 2px solid #379FF6;
}
/* 导航栏样式结束 */
/* 轮播样式 */
.play{
  width: 720rpx;
  height: 330rpx;
  margin: 0 15rpx 0 15rpx;
}
.play swiper{
  width: 100%;
  height: 100%;
}
.play-img{
  width: 100%;
  height: 330rpx;
}
/* 轮播样式结束 */
/* 推荐样式 */
.recommend-title{
  width: 70%;
  margin: 25rpx 0 27rpx 30rpx;
  display: flex;
  align-items: center;
}
.recommend-img{
  width: 44rpx;
  height: 44rpx;
}
.recommend-word{
  font-size: 37.5rpx;
  font-weight: 400;
  color: #379FF6;
  margin-left: 7rpx;
}
.recommend-list{
  width: 720rpx;
  margin: 0 15rpx 0 15rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.recommend-item{
  width: 350rpx;
  margin-bottom: 28rpx;
}
.recommend-photo{
  width: 350rpx;
  height: 214rpx;
  border-radius:12rpx 12rpx 0px 0px;
}
.recommend-name{
  display: block;
  width: 344rpx;
  height: 27rpx;
  line-height: 27rpx;
  font-size: 25rpx;
  color: #292D30;
  /* 超出一行文字自动隐藏  */
  overflow: hidden;
  /* 文字隐藏后添加省略号 */
  text-overflow: ellipsis;
  /* 强制不换行 */
  white-space: nowrap;
  margin-top: 14rpx;
}
.recommend-person{
  width: 350rpx;
  height: 35rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 12rpx;
}
.recommend-author{
  display: block;
  font-size: 25rpx;
  color: #808080;
  margin-top: -2rpx;
}
.recommend-forward{
  display: block;
  width: 90rpx;
  height: 35rpx;
  padding: 0;
  margin-right: 0;
  line-height: 35rpx;
  background-color: #379FF6;
  border-radius: 6rpx;
  color: #ffffff;
  font-size: 22rpx;
  display: flex;
  justify-content: space-around;
  align-content: center;
}
.recommend-icon{
  margin-top: 9rpx;
  width: 20rpx;
  height: 18rpx;
}
/* 推荐样式结束 */
